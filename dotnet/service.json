{
  "stack": "dotnet",
  "memory": 256,
  "cpu": {
    "type": "shared",
    "value": 80
  },
  "minReplicas": 8,
  "maxReplicas": 80,
  "runtimeArgs": [
  ],
  "routes": {
    "reviewApiAction": {
      "path": "/reviews-and-ratings-demo/api/:requestedAction",
      "public": true
    },
    "reviewApiActionId": {
      "path": "/reviews-and-ratings-demo/api/:requestedAction/:id",
      "public": true
    }
  },
  "events": {
    "onAppsLinked": {
      "sender": "apps",
      "keys": ["linked"]
    }
  }
}
